<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.helicopter_service_2321523.SecurityOfficer.SecurityOfficerController"
      alignment="TOP_LEFT"
      spacing="15">

    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Label text="Security Officer Dashboard" style="-fx-font-size: 22px; -fx-font-weight: bold;"/>

    <Label fx:id="nameLabel" text="Name: " style="-fx-font-size: 16px;"/>
    <Label fx:id="userIdLabel" text="User ID: " style="-fx-font-size: 16px;"/>
    <Label fx:id="clearanceLevelLabel" text="Clearance Level: " style="-fx-font-size: 16px;"/>
    <Label fx:id="shiftTimeLabel" text="Shift Time: " style="-fx-font-size: 16px;"/>

</VBox>